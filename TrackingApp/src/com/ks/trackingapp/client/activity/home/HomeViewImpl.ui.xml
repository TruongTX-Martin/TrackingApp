<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:v="urn:import:com.ks.trackingapp.client.view">
	<ui:style>
	.important {
		font-weight: bold;
	}
	
	.panelScroll {
		overflow: hidden;
		background-color: #2C434D;
	}
	</ui:style>
	<g:FlowPanel>
		<v:KSScrollPanel ui:field="panelScroll" styleName="{style.panelScroll}">
			<g:FlowPanel ui:field="flowComment">
			</g:FlowPanel>
		</v:KSScrollPanel>
	</g:FlowPanel>
</ui:UiBinder> 