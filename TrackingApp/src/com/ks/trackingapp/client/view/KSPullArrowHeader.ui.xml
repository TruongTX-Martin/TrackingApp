<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:com.googlecode.mgwt.ui.client.widget.button">
	<ui:style>
		.loadingViewPanel {
			z-index: 99999;
		}
		
		.contentPanel {
			position: relative;	
			background-repeat: no-repeat;
				background-size: 100% 100%;
		}
		
		.loadingText {
			color: #00a99d;
			font-size: 1.3em;
		}
		
		.imageMusicNote {
			height: 20px;
			margin: 5px;
			float: left;
		}
		
		.progressLoading { 
			position: relative;
			display: inline-block;
		}
	</ui:style>
	<g:VerticalPanel styleName="{style.loadingViewPanel}" ui:field="mainPanel" spacing="0" width="100%" height="100%" horizontalAlignment="ALIGN_CENTER"  verticalAlignment="ALIGN_MIDDLE">
				<g:FlowPanel ui:field="loadingPanel" styleName="{style.progressLoading}" height="10px">
					<!-- <g:Image ui:field="image1" styleName="{style.imageMusicNote}"  url="images/char-little.png"></g:Image>
					<g:Image ui:field="image2" styleName="{style.imageMusicNote} imageMusicNote"  url="images/char-z.png"></g:Image>
					<g:Image ui:field="image3" styleName="{style.imageMusicNote} imageMusicNote"  url="images/char-e.png"></g:Image>
					<g:Image ui:field="image4" styleName="{style.imageMusicNote} imageMusicNote"  url="images/char-r.png"></g:Image>
					<g:Image ui:field="image5" styleName="{style.imageMusicNote} imageMusicNote"  url="images/char-o.png"></g:Image>
					<g:Image ui:field="image6" styleName="{style.imageMusicNote} imageMusicNote"  url="images/char-s.png"></g:Image> -->
					 <g:HTML styleName="bullet"></g:HTML>
					   <g:HTML styleName="bullet"></g:HTML>
					    <g:HTML styleName="bullet"></g:HTML>
					     <g:HTML styleName="bullet"></g:HTML>
					    
				</g:FlowPanel>
				<g:HTML ui:field="htmlLoading" addStyleNames="{style.loadingText}"></g:HTML>
	</g:VerticalPanel>
</ui:UiBinder> 