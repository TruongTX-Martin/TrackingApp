<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:v="urn:import:com.ks.trackingapp.client.view"
	xmlns:gwtbootstrap3="urn:import:org.gwtbootstrap3.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	
	.mailPanel {
		width: 200px;
		height: 360px;
		vertical-align: middle;
		background-color: #2B444B;
		border: 1px solid #ffffff;
	}
	
	.flowAll {
		width: 100%;
	}
	
	.htmlAll {
		color: #ffffff;
		font-size: 15px;
		font-weight: bold;
		margin-left: 10px;
		margin-top: 9px;
		margin-bottom: 9px;
		display: inline-block;
		width: 70%;
	}
	
	.flowAndroid {
		width: 100%;
	}
	
	.htmlAndroid {
		color: #ffffff;
		font-size: 15px;
		font-weight: bold;
		margin-left: 10px;
		margin-top: 9px;
		margin-bottom: 9px;
		display: inline-block;
		width: 70%;
	}
	
	.flowIOS {
		width: 100%;
	}
	
	.htmlIOS {
		color: #ffffff;
		font-size: 15px;
		font-weight: bold;
		margin-left: 10px;
		margin-top: 9px;
		margin-bottom: 9px;
		display: inline-block;
		width: 70%;
	}
	
	.flowsaparator {
		width: 100%;
		height: 1px;
		background-color: #ffffff;
	}
	
	.imgLanguage {
		display: inline-block;
		width: 30%;
		float: left;
		margin-top: 7px;
		margin-left: 3px;
	}
	</ui:style>


	<g:FlowPanel styleName="{style.mailPanel}">

		<v:VerticalTouchPanel ui:field="touchEnglish"
			width="100%">
			<g:FlowPanel styleName="{style.flowAll}">
				<gwtbootstrap3:Image ui:field="imgEnglish"
					styleName="{style.imgLanguage}"></gwtbootstrap3:Image>
				<g:HTML styleName="{style.htmlAll}" ui:field="htmlEnglish"></g:HTML>
			</g:FlowPanel>
		</v:VerticalTouchPanel>
		<g:FlowPanel styleName="{style.flowsaparator}" />
		
		<v:VerticalTouchPanel ui:field="touchFrench"
			width="100%">
			<g:FlowPanel styleName="{style.flowAll}">
				<gwtbootstrap3:Image ui:field="imgFrench"
					styleName="{style.imgLanguage}"></gwtbootstrap3:Image>
				<g:HTML styleName="{style.htmlAll}" ui:field="htmlFrench"></g:HTML>
			</g:FlowPanel>
		</v:VerticalTouchPanel>
		<g:FlowPanel styleName="{style.flowsaparator}" />

		<v:VerticalTouchPanel ui:field="touchMalay"
			width="100%">
			<g:FlowPanel styleName="{style.flowAndroid}">
				<gwtbootstrap3:Image ui:field="imgMalay"
					styleName="{style.imgLanguage}"></gwtbootstrap3:Image>
				<g:HTML styleName="{style.htmlAndroid}" ui:field="htmlMalay"></g:HTML>
			</g:FlowPanel>
		</v:VerticalTouchPanel>
		<g:FlowPanel styleName="{style.flowsaparator}" />

		<v:VerticalTouchPanel ui:field="touchIndo" width="100%">
			<g:FlowPanel styleName="{style.flowIOS}">
				<gwtbootstrap3:Image ui:field="imgIndo"
					styleName="{style.imgLanguage}"></gwtbootstrap3:Image>
				<g:HTML styleName="{style.htmlIOS}" ui:field="htmlIndonesia"></g:HTML>
			</g:FlowPanel>
		</v:VerticalTouchPanel>
		<g:FlowPanel styleName="{style.flowsaparator}" />

		<v:VerticalTouchPanel ui:field="touchPortuge"
			width="100%">
			<g:FlowPanel styleName="{style.flowIOS}">
				<gwtbootstrap3:Image ui:field="imgPortuguese"
					styleName="{style.imgLanguage}"></gwtbootstrap3:Image>
				<g:HTML styleName="{style.htmlIOS}" ui:field="htmlPortuguese"></g:HTML>
			</g:FlowPanel>
		</v:VerticalTouchPanel>
		<g:FlowPanel styleName="{style.flowsaparator}" />

		<v:VerticalTouchPanel ui:field="touchGermany"
			width="100%">
			<g:FlowPanel styleName="{style.flowIOS}">
				<gwtbootstrap3:Image ui:field="imgGermany"
					styleName="{style.imgLanguage}"></gwtbootstrap3:Image>
				<g:HTML styleName="{style.htmlIOS}" ui:field="htmlGermany"></g:HTML>
			</g:FlowPanel>
		</v:VerticalTouchPanel>
		<g:FlowPanel styleName="{style.flowsaparator}" />

		<v:VerticalTouchPanel ui:field="touchSpanish"
			width="100%">
			<g:FlowPanel styleName="{style.flowIOS}">
				<gwtbootstrap3:Image ui:field="imgSpanish"
					styleName="{style.imgLanguage}"></gwtbootstrap3:Image>
				<g:HTML styleName="{style.htmlIOS}" ui:field="htmlSpanish"></g:HTML>
			</g:FlowPanel>
		</v:VerticalTouchPanel>
		<g:FlowPanel styleName="{style.flowsaparator}" />

		<v:VerticalTouchPanel ui:field="touchChinese"
			width="100%">
			<g:FlowPanel styleName="{style.flowIOS}">
				<gwtbootstrap3:Image ui:field="imgChinese"
					styleName="{style.imgLanguage}"></gwtbootstrap3:Image>
				<g:HTML styleName="{style.htmlIOS}" ui:field="htmlChinese"></g:HTML>
			</g:FlowPanel>
		</v:VerticalTouchPanel>
		<g:FlowPanel styleName="{style.flowsaparator}" />

		<v:VerticalTouchPanel ui:field="touchVietnam"
			width="100%">
			<g:FlowPanel styleName="{style.flowIOS}">
				<gwtbootstrap3:Image ui:field="imgVietnam"
					styleName="{style.imgLanguage}"></gwtbootstrap3:Image>
				<g:HTML styleName="{style.htmlIOS}" ui:field="htmlVietname"></g:HTML>
			</g:FlowPanel>
		</v:VerticalTouchPanel>
		<g:FlowPanel styleName="{style.flowsaparator}" />


	</g:FlowPanel>

</ui:UiBinder> 